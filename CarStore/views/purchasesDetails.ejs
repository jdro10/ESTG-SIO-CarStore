        <script type="text/javascript">
            google.charts.load('current', { packages: ['corechart', 'bar'] });
            google.charts.setOnLoadCallback(drawBasic);

            function drawBasic() {

                var data = new google.visualization.DataTable();
                data.addColumn('timeofday', 'Months');
                data.addColumn('number', 'Purchases');

                data.addRows([
                    [{ v: [1, 0, 0], f: 'January' }, parseInt('<%= monthArray[0] %>')],
                    [{ v: [2, 0, 0], f: 'February' }, parseInt('<%= monthArray[1] %>')],
                    [{ v: [3, 0, 0], f: 'March' }, parseInt('<%= monthArray[2] %>')],
                    [{ v: [4, 0, 0], f: 'April' }, parseInt('<%= monthArray[3] %>')],
                    [{ v: [5, 0, 0], f: 'May' }, parseInt('<%= monthArray[4] %>')],
                    [{ v: [6, 0, 0], f: 'June' }, parseInt('<%= monthArray[5] %>')],
                    [{ v: [7, 0, 0], f: 'July' }, parseInt('<%= monthArray[6] %>')],
                    [{ v: [8, 0, 0], f: 'August' }, parseInt('<%= monthArray[7] %>')],
                    [{ v: [9, 0, 0], f: 'September' }, parseInt('<%= monthArray[8] %>')],
                    [{ v: [10, 0, 0], f: 'October' }, parseInt('<%= monthArray[9] %>')],
                    [{ v: [11, 0, 0], f: 'November' }, parseInt('<%= monthArray[10] %>')],
                    [{ v: [12, 0, 0], f: 'December' }, parseInt('<%= monthArray[11] %>')],
                ]);

                var options = {
                    title: 'Company purchases per month',
                    hAxis: {
                        textPosition: 'none'
                    },
                    vAxis: {
                        title: 'Number of purchases'
                    }
                };

                var chart = new google.visualization.ColumnChart(
                    document.getElementById('chart_div'));

                chart.draw(data, options);
            }
        </script>
